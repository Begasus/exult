<?xml version="1.0" encoding="UTF-8"?>
<Scheme>
   <BuildAction>
      <PreActions>
         <ExecutionAction
            ActionType = "Xcode.IDEStandardExecutionActionsCore.ExecutionActionType.ShellScriptAction">
            <ActionContent
               title = "delete libraries folders and clone the libraries' HEAD"
               scriptText = "rm -r -f ${WORKSPACE_PATH}/../../libogg&#10;git clone https://github.com/xiph/ogg ${WORKSPACE_PATH}/../../libogg&#10;rm -r -f ${WORKSPACE_PATH}/../../libvorbis&#10;git clone https://github.com/xiph/vorbis ${WORKSPACE_PATH}/../../libvorbis&#10;rm -r -f ${WORKSPACE_PATH}/../../libmt32emu&#10;git clone https://github.com/munt/munt ${WORKSPACE_PATH}/../../libmt32emu&#10;rm -r -f ${WORKSPACE_PATH}/../../SDL2&#10;git clone https://github.com/libsdl-org/sdl ${WORKSPACE_PATH}/../../SDL2&#10;">
            </ActionContent>
         </ExecutionAction>
      </PreActions>
      <BuildActionEntries>
         <BuildActionEntry
            buildForTesting = "YES"
            buildForRunning = "YES"
            buildForProfiling = "NO"
            buildForArchiving = "NO"
            buildForAnalyzing = "NO">
            <BuildableReference
               BuildableIdentifier = "primary"
               BuildableName = "Exult.app"
               BlueprintName = "Exult"
               ReferencedContainer = "container:Exult.xcodeproj">
            </BuildableReference>
         </BuildActionEntry>
      </BuildActionEntries>
   </BuildAction>
</Scheme>
