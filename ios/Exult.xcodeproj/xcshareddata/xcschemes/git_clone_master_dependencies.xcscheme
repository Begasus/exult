<?xml version="1.0" encoding="UTF-8"?>
<Scheme>
   <BuildAction>
      <PreActions>
         <ExecutionAction
            ActionType = "Xcode.IDEStandardExecutionActionsCore.ExecutionActionType.ShellScriptAction">
            <ActionContent
               title = "clone the libraries&apos; master"
               scriptText = "git clone https://github.com/xiph/ogg ${WORKSPACE_PATH}/../../libogg&#10;git clone https://github.com/xiph/vorbis ${WORKSPACE_PATH}/../../libvorbis&#10;git clone https://github.com/munt/munt ${WORKSPACE_PATH}/../../libmt32emu&#10;git clone https://github.com/libsdl-org/sdl ${WORKSPACE_PATH}/../../SDL2&#10;">
            </ActionContent>
         </ExecutionAction>
      </PreActions>
      <BuildActionEntries>
         <BuildActionEntry
            buildForRunning = "YES">
            <BuildableReference
               BuildableIdentifier = "primary"
               BuildableName = "Exult.app"
               BlueprintName = "Exult"
               ReferencedContainer = "container:Exult.xcodeproj">
            </BuildableReference>
         </BuildActionEntry>
      </BuildActionEntries>
   </BuildAction>
   <LaunchAction>
      <BuildableProductRunnable>
         <BuildableReference
            BuildableIdentifier = "primary"
            BuildableName = "Exult.app"
            BlueprintName = "Exult"
            ReferencedContainer = "container:Exult.xcodeproj">
         </BuildableReference>
      </BuildableProductRunnable>
   </LaunchAction>
</Scheme>
